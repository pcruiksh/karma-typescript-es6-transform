{
  "name": "@pcruiksh/karma-typescript-es6-transform",
  "version": "0.0.1",
  "description": "A fork of karma-typescript-es6-transform that uses Babel 7",
  "main": "dist/transform.js",
  "keywords": [
    "babel",
    "ES2015",
    "ES6",
    "karma-typescript",
    "typescript"
  ],
  "author": "monounity",
  "contributors": [
    "erikbarke"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pcruiksh/karma-typescript-es6-transform.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc --rootDir ./src",
    "build:ci": "tsc --noEmit --rootDir ./src",
    "build:watch": "tsc -w --rootDir ./src",
    "lint": "tslint --project tsconfig.json",
    "test": "nyc --require ts-node/register tape src/*.spec.ts | faucet"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.spec.ts"
    ],
    "reporter": [
      "html"
    ],
    "temp-directory": "./coverage/.nyc_output"
  },
  "dependencies": {
    "acorn": "^5.4.1",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "log4js": "^1.1.1",
    "magic-string": "^0.22.4"
  },
  "devDependencies": {
    "@types/acorn": "^4.0.0",
    "@types/babel-core": "^6.7.14",
    "@types/estree": "0.0.38",
    "@types/karma": "^1.7.3",
    "@types/lodash": "^4.14.55",
    "@types/log4js": "0.0.32",
    "@types/node": "^9.4.6",
    "@types/sinon": "^4.1.3",
    "@types/tape": "^4.2.29",
    "faucet": "0.0.1",
    "karma-typescript": "latest",
    "nyc": "^11.5.0",
    "sinon": "^4.4.1",
    "tape": "^4.6.3",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.0.0",
    "typescript": "latest"
  }
}
